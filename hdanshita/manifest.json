{
    "manifest_version": 3,
    "name": "hdanshita Image Replacer",
    "version": "1.0",
    "description": "Replace all images with sensei images.",
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
  }
  